    "@vitejs/plugin-vue": "^5.1.3",
    "@vitest/coverage-v8": "^2.1.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "jsdom": "^25.0.1",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.3",
